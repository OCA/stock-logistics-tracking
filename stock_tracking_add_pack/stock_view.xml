<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

	    <record id="view_tracking_form_add_pack" model="ir.ui.view">
			<field name="name">stock.tracking.form.inherit</field>
			<field name="model">stock.tracking</field>
			<field name="type">form</field>
			<field name="inherit_id" ref="stock_tracking_child.view_tracking_child_form"/>
			<field name="arch" type="xml">
				<field name="child_ids" position="after">
					<newline/>
					<button name="%(stock_tracking_add_product.action_stock_packaging_add)d" states="open" string="Add packs" colspan="1" type="action"  context="{'type_selection':'pack'}"/>
					<!--<button name="%(stock_tracking_add_product.action_stock_packaging_delete)d" colspan="1" string="Delete packs" type="action" context="{'type_selection':'pack'}"/>-->
				</field>
			</field>
		</record>

    </data>
</openerp>
