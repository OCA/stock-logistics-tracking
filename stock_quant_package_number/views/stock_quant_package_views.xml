<odoo>
    <record model="ir.ui.view" id="stock_quant_package_form_view">
        <field
            name="name"
        >stock.quant.package.form (in stock_quant_package_number)</field>
        <field name="model">stock.quant.package</field>
        <field name="inherit_id" ref="stock.view_quant_package_form" />
        <field name="arch" type="xml">
          <field name="package_type_id" position="after">
            <field name="number_packages" />
          </field>
        </field>
    </record>

    <record model="ir.ui.view" id="stock_quant_package_tree_view">
        <field
            name="name"
        >stock.quant.package.tree (in stock_quant_package_number)</field>
        <field name="model">stock.quant.package</field>
        <field name="inherit_id" ref="stock.view_quant_package_tree" />
        <field name="arch" type="xml">
          <field name="package_type_id" position="after">
            <field name="number_packages" />
          </field>
        </field>
    </record>

</odoo>
